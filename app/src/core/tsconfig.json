{
    "extends": "../../tsconfig.webpack.json",
    "compilerOptions": {
        "rootDir": ".",
        "declarationDir": "../../out/core",
        "outDir": "../../out/core",
        "typeRoots": [
            "../../../node_modules/@types"
        ],
        "baseUrl": "./",
        "paths": {
            "assets": ["assets"],
            "assets/*": ["assets/*"],
            "elements": ["elements"],
            "elements/*": ["elements/*"],
            "classes/*": ["classes/*"],
            "collections/*": ["collections/*"],
            "components/*": ["components/*"],
            "pages/*": ["pages/*"],
            "styling/*": ["styling/*"],
            "stores": ["stores/index.tsx"],
            "stores/*": ["stores/*"],
            "router": ["router"],
            "router/*": ["router/*"],
            "menus": ["menus/index.ts"],
            "menus/*": ["menus/*"],
            "utils/*": ["utils/*"],
            "decorators": ["decorators.tsx"],
            "interfaces": ["interfaces.ts"],
            "ioc": ["ioc.ts"]
        }
    },
    "include": [
        "index.tsx"
    ],
    "exclude": [
        "node_modules",
        "_small.tsx"
    ]
}
